{
    "name": "medidash/phpsdk",
    "description": "Medidash Application Api Wrapper",
    "type": "library",
    "require": {
        "laminas/laminas-http": "^2.19.0",
        "laminas/laminas-config": "^3.9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "10.5.x-dev",
        "squizlabs/php_codesniffer": "4.0.x-dev",
        "phpstan/phpstan": "2.0.x-dev"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Medidash\\Phpsdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Medidash\\Phpsdk\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "check": [
            "@test"
        ],
        "test": "phpunit"
    },
    "authors": [
        {
            "name": "Bona Philippe",
            "email": "bona.lukengu@mukuru.com"
        }
    ],
    "minimum-stability": "dev"
}
